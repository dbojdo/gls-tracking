{
    "name" : "webit/gls-tracking",
    "type" : "library",
    "description": "Web-IT GLS Tracking API",
    "keywords": ["gls", "tracking", "track", "trace", "parcel"],
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "webit/soap-api": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\GlsTracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\GlsTracking\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
